def sum(*n):
    total = 0
    for n1 in n:
        total = total + n1
    print('the sum=', total)


sum()
sum(10)
sum(10, 20)
sum(10, 20, 30)


# [EXAMPLE 2]
def f(*s, n1):
    for s1 in s:
        print(s1)
    print(n1)


f('a', 'b', n1=10)


def f(n1, *args):
    print(n1)
    for s in args:
        print(s)


f(10)
f(20, 30)
f('a', 10, 'b', 30)

