height=float(input('enter height in meters : '))
weight=float(input('enter weight in kg : '))

bmi=weight/(height**2)

print('your bmi is: {0} and you are :'.format(bmi),end='')

if bmi>16:
    print('severely under weight')

elif 16 <= bmi < 18.5:
    print('underweight')

elif 18.5 <= bmi < 25:
    print('healthy')

elif 25 <= bmi < 30:
    print('overweight')

elif bmi>=30:
    print('severely overweight')
