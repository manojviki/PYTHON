print('calculator')
print('1.add')
print('2.subtract')
print('3.multiply')
print('4.divide')

a = int(input('enter a : '))
b = int(input('enter b : '))
choice = int(input('enter your choice(1-4) : '))

if choice == 1:
    c = a + b
    print('sum= ', c)

elif choice == 2:
    c = a - b
    print('difference= ', c)

elif choice == 3:
    c = a * b
    print('product= ', c)

elif choice == 4:
    c = a / b
    print('quatient =', c)

else:
    print('Invalid choice')
